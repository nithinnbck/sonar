pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'jenkins-sonar') { 
          bat '''echo "Absolute Folder "'''
          bat '''sonar-scanner -Dsonar.projectKey=python -Dsonar.sources=.'''
        }
      }
    }
  }
}
